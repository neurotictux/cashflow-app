{
  "scripts": {
    "start": "babel-node src/run.js",
    "dev": "nodemon --watch ./src --exec babel-node src/run.js",
    "test": "npm run pre-test && jest --runInBand --coverage",
    "pre-test": "node before.test.js && npm run migrate && npm run seed",
    "migrate": "sequelize db:migrate --config=./src/config.js --options-path=./src/sequelize/options.json",
    "seed": "sequelize db:seed:all --config=./src/config.js --options-path=./src/sequelize/options.json",
    "lint": "eslint src --fix"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "mod-cross-cutting": "file:../mod-cross-cutting",
    "moment": "^2.24.0",
    "pg": "^7.9.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^5.6.0",
    "sha1": "^1.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "babel-jest": "^24.7.1",
    "dotenv": "^7.0.0",
    "eslint": "^5.16.0",
    "jest": "^24.7.1",
    "nodemon": "^1.18.11",
    "sequelize-cli": "^5.4.0",
    "sqlite3": "^4.0.6",
    "superagent": "^5.0.2",
    "supertest": "^4.0.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ]
  }
}
